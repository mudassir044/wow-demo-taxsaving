    <script>
        // Initialize animations
        AOS.init({
            duration: 800,
            easing: 'ease-in-out',
            once: true
        });

        // Employee count calculator
        const employeeSlider = document.getElementById('employees');
        const employeeCount = document.getElementById('employeeCount');
        const annualSavings = document.getElementById('annualSavings');
        const monthlySavings = document.getElementById('monthlySavings');

        employeeSlider.addEventListener('input', function() {
            const count = this.value;
            employeeCount.textContent = count;
            
            const savings = count * 681.60;
            annualSavings.textContent = '$' + savings.toLocaleString();
            
            const monthly = savings / 12;
            monthlySavings.textContent = '$' + monthly.toLocaleString(undefined, {maximumFractionDigits: 2});
        });

        // Feather icons
        feather.replace();
    </script>
</body>
